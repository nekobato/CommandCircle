{
  "name": "command-circle",
  "private": true,
  "version": "0.0.14",
  "author": {
    "name": "nekobato",
    "email": "nekobato@gmail.com",
    "url": "https://nekobato.net"
  },
  "license": "MIT",
  "description": "Ring UI Application Launcher for macOS",
  "scripts": {
    "dev": "vite",
    "build": "node script/copyBinary && vue-tsc && vite build && electron-builder --config electron-builder.config.cjs",
    "release": "node script/copyBinary && vite build && electron-builder --publish always --config electron-builder.config.cjs",
    "build:icon": "electron-icon-builder --input=./public/icon.png --output=./public"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "dependencies": {
    "electron-store": "^8.2.0",
    "file-icon": "^5.1.1"
  },
  "devDependencies": {
    "@electron/notarize": "^2.3.0",
    "@iconify/vue": "^4.1.1",
    "@sentry/electron": "^4.22.0",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^4.6.2",
    "electron": "^28.2.10",
    "electron-builder": "^24.13.3",
    "electron-icon-builder": "^2.0.1",
    "electron-log": "^5.1.2",
    "electron-updater": "^6.1.8",
    "fs-extra": "^11.2.0",
    "nanoid": "^5.0.7",
    "sass": "^1.74.1",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-tsc": "^1.8.27"
  },
  "main": "dist-electron/main.js"
}
