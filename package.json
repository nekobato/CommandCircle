{
  "name": "round-drop",
  "private": true,
  "version": "1.1.1",
  "author": {
    "name": "nekobato",
    "email": "nekobato@gmail.com",
    "url": "https://nekobato.net"
  },
  "license": "MIT",
  "description": "Ring UI Application Launcher for macOS",
  "scripts": {
    "dev": "vite",
    "build": "node script/copyBinary && vue-tsc && vite build && electron-builder --config electron-builder.config.cjs",
    "release": "node script/copyBinary && vite build && electron-builder --publish always --config electron-builder.config.cjs",
    "build:icon": "electron-icon-builder --input=./public/icon.png --output=./public"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "dependencies": {
    "electron-store": "^10.0.0",
    "file-icon": "^5.1.1"
  },
  "devDependencies": {
    "@electron/notarize": "^2.3.2",
    "@iconify/vue": "^4.1.2",
    "@sentry/electron": "^5.1.0",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vueuse/integrations": "^10.11.0",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "electron-icon-builder": "^2.0.1",
    "electron-log": "^5.1.5",
    "electron-updater": "^6.2.1",
    "fs-extra": "^11.2.0",
    "nanoid": "^5.0.7",
    "sass": "^1.77.6",
    "sortablejs": "^1.15.2",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.29",
    "vue-tsc": "^2.0.21"
  },
  "main": "dist-electron/main.js"
}
